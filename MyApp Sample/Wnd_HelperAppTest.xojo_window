#tag WindowCode
	#tag Event
		Sub Open()
		  // On positionne l'état de la préférence pour l'ouverture automatique
		  // We put checkBox state from the preferences
		  
		  CB_OpenAuto.Value = OpenAuto
		End Sub
	#tag EndEvent


#tag EndWindowCode

#tag Events CB_OpenAuto
	#tag Event
		Sub Action()
		  // On enregistre la préférence de l'état du lancement auto.
		  // We save preference state of AutoLauch
		  SavePrefs("OpenAuto",Str(me.Value))
		  OpenAuto = me.Value
		  
		  // On test ici si notre helper est enregistré par le systeme.
		  // We check that our App is registered by System
		  if ServiceManagementModuleMBS.LoginItemSetEnabled("fr.vdsc.helpertest", True) then
		    // Real name of the App
		    if ServiceManagementModuleMBS.RegisterHelperApp("Helper-Test.app", True) then 
		      
		    end if
		    
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
